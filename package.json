{
  "name": "vite-project",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev vite --config ./config/vite.config.dev.ts --host",
    "dev:test": "cross-env NODE_ENV=test vite --config ./config/vite.config.dev.ts --host",
    "dev:https": "cross-env NODE_ENV=dev vite --host",
    "build": "cross-env NODE_ENV=prod run-p build-only",
    "build:test": "cross-env NODE_ENV=test run-p type-check build-only",
    "preview": "cross-env NODE_ENV=dev vite preview",
    "report": "rimraf dist && vite build",
    "build-only": "rimraf dist && vite build --config ./config/vite.config.prod.ts",
    "type-check": "vue-tsc --noEmit",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.4.2",
    "amfe-flexible": "^2.2.1",
    "axios": "^1.4.0",
    "crypto-js": "^4.1.1",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.9",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "pinia": "2.1.6",
    "pinia-plugin-persistedstate": "^3.1.0",
    "qrcode.vue": "^3.4.0",
    "vant": "^4.6.3",
    "vconsole": "^3.15.1",
    "vue": "^3.3.4",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.2.4",
    "vue-vconsole-devtools": "^1.0.9",
    "vue3-seamless-scroll": "^2.0.1"
  },
  "devDependencies": {
    "@plugin-web-update-notification/vite": "^1.6.5",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^10.2.1",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.3",
    "terser": "^5.19.2",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-top-level-await": "^1.3.1",
    "vue-tsc": "^1.8.5"
  }
}
