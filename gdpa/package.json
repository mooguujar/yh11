{
  "name": "vite-project",
  "private": true,
  "version": "1.3.20",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev DEBUG=\"vite:transform\" vite --profile --config ./config/vite.config.dev.ts --host",
    "dev-debug": "NODE_OPTIONS='--inspect-brk' run-p dev",
    "dev:test": "cross-env NODE_ENV=test vite --config ./config/vite.config.dev.ts --host",
    "dev:https": "cross-env NODE_ENV=dev vite --host",
    "build": "cross-env NODE_ENV=prod run-p build-only",
    "build:pre-release": "cross-env NODE_ENV=pre-release run-p build-only",
    "build:pre-release:dev": "rimraf dist && cross-env NODE_ENV=pre-release vite build --config ./config/vite.config.prod.ts",
    "preview": "cross-env NODE_ENV=dev vite preview",
    "report": "rimraf dist && vite build --config ./config/vite.config.prod.ts",
    "build-only": "rimraf dist && vite build --config ./config/vite.config.prod.ts",
    "type-check": "vue-tsc --noEmit",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "release:version": "release-it"
  },
  "release-it": {
    "git": {
      "commitMessage": "release: GDPAY-H5 release v${version}"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh .husky/pre-commit",
      "post-commit": "sh .husky/post-commit"
    }
  },
  "dependencies": {
    "@fingerprintjs/fingerprintjs": "^3.4.2",
    "@vant/touch-emulator": "^1.4.0",
    "@vant/use": "^1.6.0",
    "amfe-flexible": "^2.2.1",
    "axios": "^1.6.7",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "html5-qrcode": "^2.3.8",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "pinia": "2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qrcode": "1.5.3",
    "qrcode.vue": "^3.4.1",
    "vant": "^4.8.4",
    "vconsole": "^3.15.1",
    "vue": "^3.4.19",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.2.5",
    "vue-vconsole-devtools": "^1.0.9",
    "vue-wechat-title": "^2.0.7",
    "vue3-seamless-scroll": "^2.0.1",
    "vxe-table": "^4.5.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@plugin-web-update-notification/vite": "^1.7.1",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "4.17.9",
    "@types/node": "^18.19.17",
    "@types/qrcode": "1.5.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-legacy": "^5.3.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "@vueuse/core": "^10.7.2",
    "autoprefixer": "^10.4.17",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-import": "^15.1.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.71.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.27.1",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^5.1.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-top-level-await": "^1.4.1",
    "vue-tsc": "^1.8.27"
  }
}
